{
  "name": "gulp-verb",
  "version": "0.2.3",
  "description": "Gulp plugin for Verb, the zero-configuration markdown docs generator for Node.js.",
  "author": {
    "name": "Brian Woodward",
    "url": "https://github.com/doowb"
  },
  "homepage": "https://github.com/assemble/gulp-verb",
  "repository": {
    "type": "git",
    "url": "https://github.com/assemble/gulp-verb.git"
  },
  "bugs": {
    "url": "https://github.com/assemble/gulp-verb/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "scripts": {
    "test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.2.10"
  },
  "keywords": [
    "docs",
    "documentation",
    "gulpplugin",
    "markdown",
    "verb"
  ],
  "dependencies": {
    "gulp-util": "^2.2.14",
    "lodash": "^2.4.1",
    "verb": "^0.2.6"
  },
  "devDependencies": {
    "coveralls": "^2.10.0",
    "event-stream": "^3.1.5",
    "istanbul": "^0.2.8",
    "mocha": "^1.18.2",
    "mocha-lcov-reporter": "0.0.1",
    "should": "^3.3.1"
  }
}
